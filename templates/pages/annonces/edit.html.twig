{% extends 'base.html.twig' %}

{% block title %} Modification une Annonce{% endblock %}

{% block body %}
<div class="container">

    <h1 class="mt-4">Modifier d'une Annonce</h1>
   

    {{ form_start(form) }}
    <div class="form-group">
        {{ form_label(form.title)}}
        {{ form_widget(form.title)}}
        <div class="form_errors">
            {{ form_errors(form.title) }}
        </div>
    </div>

    <div class="form-group">
        {{ form_row(form.submit)}}
    </div>
    {{ form_end(form) }}
</div>

{% endblock %}